{
  "name": "boilerplate-isomorphic",
  "version": "0.1.2",
  "description": "React.js boilerplate with Redux Design pattern. Application composition: wire.js",
  "scripts": {
    "slate": "rimraf -rf node_modules && npm install",
    "clean": "rimraf -rf dist",
    "start": "node src/server/index.js ",
    "dev": "set NODE_ENV=development&&nodemon src/server/index.js --progress --colors --profile",
    "prod": "npm run build&&set NODE_ENV=production&&node src/server/index.js --progress --colors --profile",
    "build": "npm run clean&&set NODE_ENV=production&&webpack -p --progress --colors --profile",
    "test": "set NODE_ENV=test ** mocha --compilers js:babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/designeng/boilerplate-isomorphic.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.7.0",
    "babel": "^5.8.21",
    "classnames": "^2.1.5",
    "essential-wire": "git://github.com/designeng/essential-wire",
    "express": "^4.13.3",
    "falcor": "^0.1.15",
    "falcor-router": "^0.2.12",
    "falcor-express": "git://github.com/designeng/falcor-express.git#request-to-context-check-array",
    "history": "1.13.0",
    "nodemon": "^1.8.1",
    "radium": "^0.15.3",
    "react": "^0.13.3",
    "react-redux": "^2.1.2",
    "react-router": "^1.0.0-rc1",
    "redux": "^3.0.0",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^0.1.0",
    "redux-undo": "^0.5.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.0",
    "babel-runtime": "^5.8.20",
    "css-loader": "^0.9.0",
    "expect": "^1.12.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "0.8.5",
    "jsdom-no-contextify": "^3.1.0",
    "merge": "^1.2.0",
    "mocha": "^2.1.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.1.0",
    "redux-devtools": "^2.1.5",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-filter-actions": "^1.0.0",
    "redux-devtools-filterable-log-monitor": "^0.4.0",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-logger": "^2.0.2",
    "rimraf": "^2.5.0",
    "style-loader": "^0.8.0",
    "url-loader": "^0.5.6",
    "webpack": "1.4.13",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
